      </attr>
      <attr floatlength="0" name="ROLE_NAME" maxlength="100" datatype="String" type="COL">
        <mapingcol datatype="VARCHAR2">ROLE_NAME</mapingcol>
      </attr>
      <attr floatlength="0" name="ROLE_DES" maxlength="500" datatype="String" type="COL">
        <mapingcol datatype="VARCHAR2">ROLE_DES</mapingcol>
      </attr>
      <attr floatlength="0" name="ROLE_STATE" maxlength="2" datatype="String" type="COL">
        <mapingcol datatype="VARCHAR2">ROLE_STATE</mapingcol>
      </attr>
      <attr floatlength="0" name="CREATE_DATE" maxlength="7" datatype="DateTime" type="COL">
        <mapingcol datatype="DATE">CREATE_DATE</mapingcol>
      </attr>
      <attr floatlength="0" name="CREATE_USERID" maxlength="32" datatype="String" type="COL">
        <mapingcol datatype="VARCHAR2">CREATE_USERID</mapingcol>
      </attr>
