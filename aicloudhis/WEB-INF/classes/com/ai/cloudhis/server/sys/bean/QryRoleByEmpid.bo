      </attr>
      <attr floatlength="0" name="ORGID_HOSP" maxlength="32" datatype="String" type="COL">
        <mapingcol datatype="VARCHAR2">ORGID_HOSP</mapingcol>
      </attr>
      <attr floatlength="0" name="EMPID" maxlength="32" datatype="String" type="COL">
