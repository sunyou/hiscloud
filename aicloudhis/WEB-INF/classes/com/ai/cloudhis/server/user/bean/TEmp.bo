      </attr>
      <attr floatlength="0" name="EMP_ADDR" maxlength="255" datatype="String" type="COL" remark="人员地址">
        <mapingcol datatype="VARCHAR2">EMP_ADDR</mapingcol>
      </attr>
      <attr floatlength="0" name="ORGID_HOSP" maxlength="50" datatype="String" type="COL" remark="所属诊所">
