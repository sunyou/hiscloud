        <mapingcol datatype="VARCHAR2">EMPID</mapingcol>
      </attr>
      <attr floatlength="0" name="EMP_NAME" maxlength="255" datatype="String" type="COL" remark="人员名称">
        <mapingcol datatype="VARCHAR2">EMP_NAME</mapingcol>
      </attr>
      <attr floatlength="0" name="LOGIN_NAME" maxlength="50" datatype="String" type="COL" remark="登陆用户名">
        <mapingcol datatype="VARCHAR2">LOGIN_NAME</mapingcol>
      </attr>
      <attr floatlength="0" name="INPUTSTR" maxlength="50" datatype="String" type="COL" remark="姓名输入符">
        <mapingcol datatype="VARCHAR2">INPUTSTR</mapingcol>
      </attr>
      <attr floatlength="0" name="EMP_TYPEID" maxlength="50" datatype="String" type="COL" remark="人员类型">
        <mapingcol datatype="VARCHAR2">EMP_TYPEID</mapingcol>
      </attr>
      <attr floatlength="0" name="EMP_ITLEID" maxlength="50" datatype="String" type="COL" remark="人员职称">
